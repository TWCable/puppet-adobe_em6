<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Page">
    <jcr:content
        cq:lastModifiedBy="admin"
        cq:template="<%= @cq_template %>"
        jcr:description="Puppet created Agent. <%= @jcr_description %>"
        jcr:primaryType="nt:unstructured"
        jcr:title="<%= @title %>"
        enabled="<%= @queue_enabled %>"
        logLevel="<%= @log_level %>"
        protocolHTTPExpired="<%= @protocol_http_expired %>"
        retryDelay="<%= @retry_delay %>"
        reverseReplication="{Boolean}<%= @reverse_replication %>"
<% if @reverse_replication == 'true' -%>
        protocolHTTPMethod="<%= @protocol_http_method %>"
<% end -%>
        serializationType="durbo"
        sling:resourceType="<%= @sling_resource_type %>"
        transportPassword="<%= @transport_password %>"
        transportUri="<%= @transport_uri %>"
        transportUser="<%= @transport_user %>"/>
</jcr:root>
